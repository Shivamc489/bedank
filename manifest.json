{
  "manifest_version": 3,
  "name": "Dank AI",
  "version": "1.0",
  "minimum_chrome_version": "128.0",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png",
    "512": "assets/icon512.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.x.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
